{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": 0,
    "no-new": 0,
    "camelcase": 0,
    "no-console": 0,
    "func-names": 0,
    "import/named": 0,
    "no-return-assign": 0,
    "arrow-body-style": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-one-expression-per-line": 0,
    "jsx-a11y/click-events-have-key-events": 0
  }
}
